{% for object in loggedObjects %}
    {#Build the object name#}
    {% set dsoName = '' %}
    {% if object.name %}
        {% set dsoName = object.name |trim('"') %}
    {% endif %}

    {% if object.cat1  %}
        {% set paranthesisOpen = '' %}
        {% if dsoName %}
            {% set dsoName = dsoName ~ ' (' ~ object.cat1 ~ ' ' ~ object.id1 %}
            {% set paranthesisOpen = 'yes' %}
        {% else %}
            {% set dsoName = object.cat1 ~ ' ' ~  object.id1 %}
        {% endif %}

        {% if object.cat2  %}
            {% if paranthesisOpen %}
                {% set dsoName = dsoName ~ ', ' ~ object.cat2 ~ ' ' ~  object.id2 ~ ')' %}
            {% else %}
                {% set dsoName = dsoName ~ ' (' ~ object.cat2 ~ ' ' ~  object.id2 ~ ')' %}
            {% endif %}
        {% endif %}
    {% endif %}

    {{ dsoName }}
    {% if dsoName %} {#TODO: Check what's happening because some entries are't displayed -> they have no names!#}
        {% if not loop.last %}, {% endif %}
    {% endif %}
{% endfor %}
